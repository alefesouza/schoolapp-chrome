{
	"name": "RebuApp",
	"description": "Extens\u00e3o do RebuApp para Google Chrome, receba notifica\u00e7\u00f5es em tempo real e tenha mais facilidade para acessar o aplicativo.",
	"version": "1.0.3.1",
	"manifest_version": 2,
	"permissions": [ "http://apps.aloogle.net/web/rebuapp", "storage", "notifications" ],
	"options_page": "paginas/opcoes.html",
	"background": {
	"page":
		"background.html"
	},
	"icons": {
		"16": "icons/icon_16.png",
		"32": "icons/icon_32.png",
		"48": "icons/icon_48.png",
		"64": "icons/icon_64.png",
		"128": "icons/icon_128.png"
	},
	"omnibox": { 
		"keyword" : "rp" 
	},
	"commands": {
		"_execute_browser_action": {
			"suggested_key": {
				"default": "Shift+Alt+R"
			}
		}
	},
	"browser_action": {
		"default_icon": {
            "19": "icons/icon.png",
            "38": "icons/icon_38.png"
        },
		"default_title": "RebuApp",
		"default_popup": "popup.html"
	}
}
